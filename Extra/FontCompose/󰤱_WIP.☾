atom = 󰆴w? ↷ (macadd ∨ macref ∨ results ∨ style ∨ p_looper ∨ p_concat ∨ lookup)

macadd = 󰆴'¡' (󰆴W? ↷ (󰆴',' ⯆ word)) concat
macref = 󰆴'⟨' 󰆴W? ↷ (
      (macref_tag  =(󰆴@ word))
    ∨ (macref_notag=(   word))
) 󰆴'⟩'

results = (
    (󰆴(W? '󰅁') (󰆴W ∨ str ∨ ~‹[^󰅂]›)* 󰆴'󰅂')
   ∨(󰆴⟶        (󰆴w ∨ str ∨ ~‹[^␠⟧⁆⟩\n\t ]›)*)
)

style = 󰆴'⦑' (󰆴W? ↷ (⠶tuple ∨ ✓)) 󰆴'⦒'
tuple = ((| (| ✓)*)? chain)+
pair  = ƨ(word) 󰆴W? ⦑ ⠶tuple? ⦒
chain = (pair ∨ word)+ 󰆴W?

concat = (⠶atom)+
p_looper = 󰆴'⟦' (󰆴W ∨ ⠶atom)* 󰆴'⟧'
p_concat = 󰆴'⁅' (󰆴W ∨ ⠶atom)* 󰆴'⁆'

lookup = str ∨ ƨ(chr ∨ '|' ∨ ',')
word   = ƨ(str ∨ chr+)
str    = ƨ(⠶esc ∨ (󰆴'"' ((esc ∨ ~‹[^"]›)*) 󰆴'"'))
esc    = 󰆴‹␛␛› ~‹.?›

chr = ¬bad ~‹.›
bad = ~‹["␠⟨⟩⟦⟧⁅⁆⦑⦒¡,|\t\n ]›

w = ~‹[ \t]+›
W = ~‹[ \t\n␠]+›

(⟦=󰆴(W?↷'⟦'))(⟧=󰆴(W?↷'⟧'))(⁅=󰆴(W?↷'⁅'))(⁆=󰆴(W?↷'⁆'))
(⦑=󰆴(W?↷'⦑'))(⦒=󰆴(W?↷'⦒'))(⟨=󰆴(W?↷'⟨'))(⟩=󰆴(W?↷'⟩'))

symmap = {
        ␛(: "parenleft"  , ␛): "parenright"  ,
        ␛{: "braceleft"  , ␛}: "braceright"  ,
        ␛[: "bracketleft", ␛]: "bracketright",
        ␛<: "less"       , ␛>: "greater"     ,
